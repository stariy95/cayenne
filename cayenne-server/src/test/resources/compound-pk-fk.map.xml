<data-map xmlns="http://cayenne.apache.org/schema/10/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/10/modelMap http://cayenne.apache.org/schema/10/modelMap.xsd"
	 project-version="10">
	<property name="defaultPackage" value="org.apache.cayenne.testdo.compoundpk"/>
	<db-entity name="compound_orders">
		<db-attribute name="order_number" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="value" type="VARCHAR" length="10"/>
	</db-entity>
	<db-entity name="compound_payments">
		<db-attribute name="amount" type="NUMERIC"/>
		<db-attribute name="order_number" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="payment_number" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
	</db-entity>
	<obj-entity name="OrderMasterPk" className="org.apache.cayenne.testdo.compoundpk.OrderMasterPk" dbEntityName="compound_orders">
		<obj-attribute name="orderNumber" type="java.lang.Integer" db-attribute-path="order_number"/>
		<obj-attribute name="value" type="java.lang.String" db-attribute-path="value"/>
	</obj-entity>
	<obj-entity name="OrderNoPK" className="org.apache.cayenne.testdo.compoundpk.OrderNoPK" dbEntityName="compound_orders">
		<obj-attribute name="value" type="java.lang.String" db-attribute-path="value"/>
	</obj-entity>
	<obj-entity name="OrderPk" className="org.apache.cayenne.testdo.compoundpk.OrderPk" dbEntityName="compound_orders">
		<obj-attribute name="orderNumber" type="java.lang.Integer" db-attribute-path="order_number"/>
		<obj-attribute name="value" type="java.lang.String" db-attribute-path="value"/>
		<post-load method-name="onPostLoad"/>
	</obj-entity>
	<obj-entity name="OrderPkFk" className="org.apache.cayenne.testdo.compoundpk.OrderPkFk" dbEntityName="compound_orders">
		<obj-attribute name="orderNumber" type="java.lang.Integer" db-attribute-path="order_number"/>
		<obj-attribute name="value" type="java.lang.String" db-attribute-path="value"/>
	</obj-entity>
	<obj-entity name="PaymentMasterPk" className="org.apache.cayenne.testdo.compoundpk.PaymentMasterPk" dbEntityName="compound_payments">
		<obj-attribute name="amount" type="java.math.BigDecimal" db-attribute-path="amount"/>
	</obj-entity>
	<obj-entity name="PaymentNoPk" className="org.apache.cayenne.testdo.compoundpk.PaymentNoPk" dbEntityName="compound_payments">
		<obj-attribute name="amount" type="java.math.BigDecimal" db-attribute-path="amount"/>
	</obj-entity>
	<obj-entity name="PaymentPk" className="org.apache.cayenne.testdo.compoundpk.PaymentPk" dbEntityName="compound_payments">
		<obj-attribute name="amount" type="java.math.BigDecimal" db-attribute-path="amount"/>
		<obj-attribute name="paymentNumber" type="java.lang.Integer" db-attribute-path="payment_number"/>
	</obj-entity>
	<obj-entity name="PaymentPkFk" className="org.apache.cayenne.testdo.compoundpk.PaymentPkFk" dbEntityName="compound_payments">
		<obj-attribute name="amount" type="java.math.BigDecimal" db-attribute-path="amount"/>
		<obj-attribute name="orderNumber" type="java.lang.Integer" db-attribute-path="order_number"/>
		<obj-attribute name="paymentNumber" type="java.lang.Integer" db-attribute-path="payment_number"/>
	</obj-entity>
	<db-relationship name="payments" source="compound_orders" target="compound_payments" toDependentPK="true" toMany="true">
		<db-attribute-pair source="order_number" target="order_number"/>
	</db-relationship>
	<db-relationship name="order" source="compound_payments" target="compound_orders" toMany="false">
		<db-attribute-pair source="order_number" target="order_number"/>
	</db-relationship>
	<obj-relationship name="payments" source="OrderMasterPk" target="PaymentMasterPk" deleteRule="Deny" db-relationship-path="payments"/>
	<obj-relationship name="payments" source="OrderNoPK" target="PaymentNoPk" deleteRule="Deny" db-relationship-path="payments"/>
	<obj-relationship name="payments" source="OrderPk" target="PaymentPk" deleteRule="Deny" db-relationship-path="payments"/>
	<obj-relationship name="payments" source="OrderPkFk" target="PaymentPkFk" deleteRule="Deny" db-relationship-path="payments"/>
	<obj-relationship name="order" source="PaymentMasterPk" target="OrderMasterPk" deleteRule="Nullify" db-relationship-path="order"/>
	<obj-relationship name="order" source="PaymentNoPk" target="OrderNoPK" deleteRule="Nullify" db-relationship-path="order"/>
	<obj-relationship name="order" source="PaymentPk" target="OrderPk" deleteRule="Nullify" db-relationship-path="order"/>
	<obj-relationship name="order" source="PaymentPkFk" target="OrderPkFk" deleteRule="Nullify" db-relationship-path="order"/>
</data-map>
